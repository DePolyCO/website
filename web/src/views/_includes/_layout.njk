      <path d="M54.043 13.621C54.043 6.095 47.997 0 40.533 0L27.02 13.621c0 7.526 6.046 13.621 13.511 13.621 7.465 0 13.511-6.095 13.511-13.62Zm60.711-.085c0 7.335-4.755 11.874-12.181 11.874h-9.409V1.662h9.409c7.426 0 12.181 4.54 12.181 11.874Zm-12.383 7.657c4.318 0 7.326-2.558 7.326-7.657 0-5.098-3.008-7.656-7.326-7.656h-4.2v15.313h4.2Zm22.43-13.433c5.226 0 8.368 3.083 8.368 8.402v1.964h-12.585c.067 2.338 1.563 4.116 4.167 4.116 2.571 0 3.612-1.304 3.881-2.354h4.184v.491c-.538 2.372-2.672 5.438-7.981 5.438-5.309 0-8.855-3.015-8.855-9.046.017-6.013 3.529-9.011 8.821-9.011Zm3.865 7.046c-.034-1.948-1.294-3.523-3.932-3.523-2.554 0-3.898 1.626-4.083 3.523h8.015Zm25.74-4.556c0 4.692-3.494 8.3-7.93 8.3h-5.864v6.877h-4.99V1.662h10.854c4.436 0 7.93 3.88 7.93 8.588Zm-8.753 4.201c2.301 0 3.83-2.05 3.83-4.2 0-2.135-1.529-4.49-3.83-4.49h-5.024v8.69h5.024Zm18.666-6.691c5.393 0 9.291 3.066 9.291 9.045 0 5.963-3.898 9.029-9.291 9.029-5.394 0-9.292-3.066-9.292-9.029 0-5.98 3.898-9.045 9.292-9.045Zm0 14.347c2.705 0 4.418-1.863 4.418-5.319 0-3.455-1.713-5.336-4.418-5.336s-4.419 1.88-4.419 5.336c0 3.456 1.73 5.319 4.419 5.319Zm16.584-20.445V25.41h-4.805V1.662h4.805Zm6.249 6.488 4.167 11.908h.286l4.167-11.908h4.889v.492l-7.746 19.242C191.638 31.07 190.109 32 186.363 32h-3.579v-3.794h4.839l.319-.271.958-2.422-6.754-16.871V8.15h5.006Z" fill="#022D42"/>
    </symbol>

    {# clip logo shape #}
    <svg viewBox="0 0 301 300">
      <clipPath id="cutout" clipPathUnits="objectBoundingBox">
        <path transform="scale(0.00332225914, 0.00333333333)" d="M255.692 44.311c58.413 58.329 58.413 152.99 0 211.39-58.413 58.399-153.026 58.399-211.44 0C14.36 225.814-.191 186.404.528 147.255L147.221.525c39.228-.719 78.578 13.9 108.471 43.786Z"/>
      </clipPath>
    </svg>

    {# clip logo shape rotate #}
    <svg viewBox="0 0 301 300">
      <clipPath id="cutout-2" clipPathUnits="objectBoundingBox">
        <path transform="scale(0.00332225914, 0.00333333333)" d="M44.322 44.31c58.343-58.413 153.027-58.413 211.369 0 58.413 58.413 58.413 153.026 0 211.439-29.893 29.893-69.312 44.445-108.471 43.726L.526 152.781C-.194 113.623 14.43 74.203 44.322 44.31Z"/>
      </clipPath>
    </svg>

    {# rarr #}
    <symbol id="rarr" fill="none" viewBox="0 0 13 8">
      <path d="m8.402 1.085 2.137 2.108H0V4.8h10.54L8.401 6.915 9.499 8 13 4.542V3.457L9.499 0 8.402 1.085Z" fill="#022D42"/>
    </symbol>

    {# larr #}
    <symbol id="larr" fill="none" viewBox="0 0 13 8">
      <path d="M4.598 1.085 2.461 3.193H13V4.8H2.46l2.138 2.115L3.501 8 0 4.542V3.457L3.501 0l1.097 1.085Z" fill="#022D42"/>
    </symbol>

    {# apostrophe #}
    <symbol id="apostrophe" fill="none" viewBox="0 0 26 23">
      <path d="M15.483 22.042V9.482c0-3.093.76-5.4 2.28-6.92 1.546-1.547 3.88-2.32 7-2.32h.88v4.68h-.56c-1.707 0-2.893.347-3.56 1.04-.667.667-1 1.84-1 3.52v1.88h4.68v10.68h-9.72ZM.883 9.482c0-3.093.773-5.4 2.32-6.92 1.546-1.547 3.88-2.32 7-2.32h.84v4.68h-.56c-1.68 0-2.867.347-3.56 1.04-.667.667-1 1.84-1 3.52v1.88h4.68v10.68H.883V9.482Z" fill="#fff"/>
    </symbol>

    {# info icon #}
    <symbol id="info-icon" fill="none" viewBox="0 0 24 24">
      <path d="M11 17h2v-6h-2v6Zm1-15C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2Zm0 18c-4.41 0-8-3.59-8-8s3.59-8 8-8 8 3.59 8 8-3.59 8-8 8ZM11 9h2V7h-2v2Z" fill="#022D42"/>
      <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2Zm0 18c-4.41 0-8-3.59-8-8s3.59-8 8-8 8 3.59 8 8-3.59 8-8 8Z" fill="#022D42"/>
    </symbol>

    {# info icon filled #}
    <symbol id="info-icon--filled" fill="none" viewBox="0 0 24 24">
      <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2Zm1 15h-2v-6h2v6Zm0-8h-2V7h2v2Z" fill="#022D42"/>
    </symbol>

    {# error icon #}
    <symbol id="err-icon" fill="none" viewBox="0 0 20 20">
      <path d="M10 20c5.52 0 10-4.48 10-10S15.52 0 10 0 0 4.48 0 10s4.48 10 10 10ZM9 5h2v6H9V5Zm0 8h2v2H9v-2Z" fill="#F66"/>
    </symbol>

    {# cross close #}
    <symbol id="cross-icon" fill="none" viewBox="0 0 10 10">
      <path d="M10 1.01 8.99 0 5 3.99 1.01 0 0 1.01 3.99 5 0 8.99 1.01 10 5 6.01 8.99 10 10 8.99 6.01 5 10 1.01Z" fill="#022D42"/>
    </symbol>

    {# tick mark #}
    <symbol id="tick-icon" fill="none" viewBox="0 0 29 21">
      <path d="M26.96 0 29 2.046 10.095 21 0 10.88l2.04-2.045 8.054 8.074L26.96 0Z" fill="#022D42"/>
    </symbol>

    {# file icon #}
    <symbol id="file-icon" opacity=".5" fill="#022D42" viewBox="0 0 22 28">
