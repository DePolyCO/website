    </symbol>

    {# dropdown arr #}
    <symbol id="dropdown-arr" fill="none" viewBox="0 0 18 11">
      <path opacity=".5" d="M2.88.453 9 6.56 15.12.453 17 2.333l-8 8-8-8L2.88.453Z" fill="#022D42" stroke="#ECECE2" stroke-width=".6"/>
    </symbol>

    {# plus icon #}
    <symbol id="plus-icon" fill="none" viewBox="0 0 24 24">
